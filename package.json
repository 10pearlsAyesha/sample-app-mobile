{
  "name": "SwagLabsMobileApp",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "eslint .",
    "clean": "react-native-clean-project",
    "clear.cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro* && rm -rf $TMPDIR/haste-*",
    "prepush": "npm run lint",
    "test": "jest",
    "android.local": "./node_modules/.bin/wdio ./tests/e2e/config/wdio.android.local.conf.js",
    "android.rdc": "./node_modules/.bin/wdio ./tests/e2e/config/wdio.android.rdc.conf.js",
    "android.release": "cd android && ./gradlew assembleRelease",
    "ios.local": "./node_modules/.bin/wdio ./tests/e2e/config/wdio.ios.local.conf.js",
    "ios.rdc": "./node_modules/.bin/wdio ./tests/e2e/config/wdio.ios.rdc.conf.js",
    "ios.clear.build": "yarn clear.cache && rm -rf ./ios/build/Build/Products/Release-iphonesimulator",
    "ios.release": "react-native run-ios --configuration Release",
    "ios.sim.build": "yarn ios.clear.build && npm run ios.release",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "i18n-js": "^3.3.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-biometrics": "^1.6.0",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-localize": "^1.1.4",
    "react-native-modal-selector": "^1.0.3",
    "react-native-quick-actions": "^0.3.12",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^5.12.1",
    "react-navigation": "^3.11.1",
    "sync-storage": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.6.2",
    "@wdio/appium-service": "^5.11.4",
    "@wdio/cli": "^5.11.10",
    "@wdio/jasmine-framework": "^5.11.0",
    "@wdio/local-runner": "^5.11.10",
    "@wdio/sauce-service": "^5.11.1",
    "@wdio/spec-reporter": "^5.11.7",
    "@wdio/sync": "^5.11.10",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-config-fb-strict": "^24.3.0",
    "eslint-config-fbjs": "^3.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-flowtype": "^3.12.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-native-clean-project": "^3.2.4",
    "react-test-renderer": "16.9.0",
    "wdio-native-app-compare-service": "^1.0.1",
    "yargs": "^14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
